<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<HBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" spacing="5" alignment="CENTER_LEFT">
    <padding>
        <Insets top="3" right="7" bottom="3" left="7"/>
    </padding>
    <Label fx:id="PinLabel" />
    <Pane HBox.hgrow="ALWAYS" />
    <Button fx:id="PinButtonGoto" text="Goto" />
    <Button fx:id="PinButtonEdit" text="Edit" />
</HBox>